{
  "presets": [
    "es2015",
    "react",
    "stage-1"
  ],
  "plugins": ["transform-es2015-spread", "transform-class-properties"],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-remove-prop-types"
      ]
    }
  }
}
